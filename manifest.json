{
    "manifest_version": 3,
    "name": "OpenStreetMap Sync",
    "version": "1.0",
    "description": "Sync OpenStreetMap views across multiple tabs",
    "permissions": [
        "storage",
        "tabs"
    ],
    "action": {
        "default_popup": "index.html"
    },
    "icons": {
        "128": "icon.png"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'"
    }
}






